<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contstraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_green"
    tools:context=".Help">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/view17"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/square_background_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="45dp"
            android:background="@drawable/lotus_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/return_from_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/arrow_button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView69"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="45dp"
            android:background="@drawable/square_background_5"
            android:fontFamily="@font/spectral_sc"
            android:gravity="center"
            android:text="@string/help"
            android:textColor="@color/text_brown"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/imageView29" />

        <TextView
            android:id="@+id/textView71"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:text="@string/faq"
            android:textColor="@color/text_brown"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textView69" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button45"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/button_background_1"
            android:fontFamily="@font/spectral_sc"
            android:text="@string/send"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintlayout10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginEnd="45dp"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/textView71">

            <TextView
                android:id="@+id/textView72"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spectral_sc"
                android:paddingStart="16sp"
                android:text="@string/question1"
                android:textColor="@color/text_brown"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@id/textView73"
                app:layout_constraintEnd_toStartOf="@id/button42"
                app:layout_constraintStart_toStartOf="@id/constraintlayout10"
                app:layout_constraintTop_toTopOf="@id/constraintlayout10" />

            <ImageButton
                android:id="@+id/button42"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Button"
                android:background="@drawable/arrow_down_icon"
                app:layout_constraintBottom_toTopOf="@id/button43"
                app:layout_constraintEnd_toEndOf="@id/constraintlayout10"
                app:layout_constraintStart_toEndOf="@id/textView72"
                app:layout_constraintTop_toTopOf="@id/constraintlayout10" />

            <TextView
                android:id="@+id/textView73"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spectral_sc"
                android:paddingStart="16sp"
                android:text="@string/question2"
                android:textColor="@color/text_brown"
                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/textView74"
                app:layout_constraintEnd_toStartOf="@id/button43"
                app:layout_constraintStart_toStartOf="@id/constraintlayout10"
                app:layout_constraintTop_toBottomOf="@+id/textView72" />

            <ImageButton
                android:id="@+id/button43"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Button"
                android:background="@drawable/arrow_down_icon"
                app:layout_constraintBottom_toTopOf="@id/button44"
                app:layout_constraintEnd_toEndOf="@id/constraintlayout10"
                app:layout_constraintStart_toEndOf="@id/textView73"
                app:layout_constraintTop_toBottomOf="@+id/button42" />

            <TextView
                android:id="@+id/textView74"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spectral_sc"
                android:paddingStart="16sp"
                android:text="@string/question3"
                android:textColor="@color/text_brown"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/constraintlayout10"
                app:layout_constraintEnd_toStartOf="@id/button44"
                app:layout_constraintStart_toStartOf="@id/constraintlayout10"
                app:layout_constraintTop_toBottomOf="@+id/textView73" />

            <ImageButton
                android:id="@+id/button44"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Button"
                android:background="@drawable/arrow_down_icon"
                app:layout_constraintBottom_toBottomOf="@id/constraintlayout10"
                app:layout_constraintEnd_toEndOf="@id/constraintlayout10"
                app:layout_constraintStart_toEndOf="@+id/textView74"
                app:layout_constraintTop_toBottomOf="@+id/button43" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintlayout11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="45dp"
            android:layout_marginEnd="45dp"
            app:layout_constraintBottom_toTopOf="@+id/button45"
            app:layout_constraintTop_toBottomOf="@id/constraintlayout10">

            <TextView
                android:id="@+id/textView75"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/square_background_5"
                android:fontFamily="@font/spectral_sc"
                android:gravity="center"
                android:text="@string/have_problems"
                android:textColor="@color/text_brown"
                android:textSize="18sp"
                app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editTextTextEmailAddress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:ems="10"
                android:fontFamily="@font/spectral_sc"
                android:hint="@string/problem"
                android:inputType="textEmailAddress"
                android:textColor="@color/black"
                android:textColorHint="@color/hint_grey"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView75" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>