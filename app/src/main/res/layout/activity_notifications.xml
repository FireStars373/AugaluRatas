<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contstraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_green"
    tools:context=".Notifications">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/view20"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:layout_marginBottom= "20dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/square_background_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/return_from_notifications"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:layout_marginStart="45dp"
            android:background="@drawable/arrow_button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="45dp"
            android:background="@drawable/lotus_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView57"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="45dp"
            android:background="@drawable/square_background_5"
            android:fontFamily="@font/spectral_sc"
            android:gravity="center"
            android:text="@string/notifications"
            android:textColor="@color/text_brown"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView27" />


        <CheckBox
            android:id="@+id/notification_post_viewed"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="40dp"
            android:layout_marginStart="45dp"
            android:buttonTint="@color/heart_grey"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView57" />

        <CheckBox
            android:id="@+id/notification_post_shared"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="60dp"
            android:layout_marginStart="45dp"
            android:buttonTint="@color/heart_grey"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/notification_post_viewed" />

        <CheckBox
            android:id="@+id/notification_post_liked"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="83dp"
            android:buttonTint="@color/heart_grey"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/notification_post_shared" />

        <CheckBox
            android:id="@+id/notification_plant_purchased"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginTop="60dp"
            android:layout_marginStart="45dp"
            android:buttonTint="@color/heart_grey"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/notification_post_liked" />

        <TextView
            android:id="@+id/textView58"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/post_viewed"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/notification_post_viewed"
            app:layout_constraintTop_toBottomOf="@+id/textView57" />

        <TextView
            android:id="@+id/textView59"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/post_viewed_description"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView58" />

        <TextView
            android:id="@+id/textView60"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/post_shared"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/notification_post_shared"
            app:layout_constraintTop_toBottomOf="@+id/textView59" />

        <TextView
            android:id="@+id/textView61"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/post_shared_description"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView60" />

        <TextView
            android:id="@+id/textView62"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/post_liked"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/notification_post_liked"
            app:layout_constraintTop_toBottomOf="@+id/textView61" />

        <TextView
            android:id="@+id/textView63"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/post_liked_description"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView62" />

        <TextView
            android:id="@+id/textView64"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/plant_bought"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/notification_plant_purchased"
            app:layout_constraintTop_toBottomOf="@+id/textView63" />

        <TextView
            android:id="@+id/textView65"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/spectral_sc"
            android:gravity="left|center_vertical"
            android:paddingStart="16sp"
            android:text="@string/plant_bought_description"
            android:textColor="@color/text_brown"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView64" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>